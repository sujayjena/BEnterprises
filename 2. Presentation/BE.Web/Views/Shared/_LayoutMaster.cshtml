    <script>
        $(function () {
            $(".knob").knob();
        });

        $(document).ready(function () {
            $(document).ajaxStart(function () {
                $.LoadingOverlay("show");
            }).ajaxStop(function () {
                $.LoadingOverlay("hide");
            });
        });

        function SucessMsgAllert(msg) {
            $("#divSuccessAlert").html('<div class="card-body"  id="divCardBody"><div class="alert alert-outline-success alert-dismissible" role="alert"><button type="button" class="close" data-dismiss="alert">&times;</button><div class="alert-icon"><i class="fa fa-check"></i></div><div class="alert-message"><span><strong>Success!</strong> ' + msg + '</span></div></div></div>')
            window.setTimeout(function () {
                $(".alert").fadeTo(500, 0).slideUp(500, function () {
                    $(this).remove();
                    $("#divCardBody").attr("display", "none");
                });
            }, 5000);
            $('.alert .close').on("click", function (e) {
                $(this).parent().fadeTo(500, 0).slideUp(500);
            });
        }
        function ErrorMsgAllert(msg) {
            $("#divErrorAlert").html('<div class="card-body"><div class="alert alert-outline-danger alert-dismissible" role="alert"><button type="button" class="close" data-dismiss="alert">&times;</button><div class="alert-icon"><i class="fa fa-times"></i></div><div class="alert-message"><span><strong>Danger!</strong> ' + msg + '</span></div></div></div>')
            window.setTimeout(function () {
                $(".alert").fadeTo(500, 0).slideUp(500, function () {
                    $(this).remove();
                });
            }, 5000);
            $('.alert .close').on("click", function (e) {
                $(this).parent().fadeTo(500, 0).slideUp(500);
            });
        }

    </script>
